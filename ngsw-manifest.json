{
  "static.ignore": [
    "^\/assets\/ignore.*$"
  ],
  "routing": {
    "index": "/index.html",
    "routes": {
      "/": {
        "match": "exact"
      }
    }
  },
  "external": {
    "urls": []
  },
  "dynamic": {
    "group": [
      {
        "name": "api",
        "urls": {
          "http://arcadia.norsys.fr/v1/": {
            "match": "prefix"
          }
        },
        "cache": {
          "optimizeFor": "performance",
          "maxAgeMs": 3600000,
          "maxEntries": 20,
          "strategy": "lru"
        }
      }
    ]
  },
  "push": {
    "showNotifications": true,
    "backgroundOnly": false
  }
}